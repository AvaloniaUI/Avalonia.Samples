    <!--  Brushes  -->
    <SolidColorBrush x:Key="HamburgerMenuHeaderBrush" Color="{DynamicResource SystemListLowColor}" />
    <SolidColorBrush x:Key="HamburgerMenuHeaderForegroundBrush" Color="{DynamicResource SystemAccentColor}" />
    <SolidColorBrush x:Key="HamburgerMenuPaneBrush" Color="{DynamicResource SystemListLowColor}" />

    <!-- Sizes -->
    <x:Double x:Key="HamburgerMenuHeaderFontSize">24</x:Double>
    <x:Double x:Key="HamburgerMenuHeaderMinHeight">48</x:Double>

    <!-- MenuItemTemplateSelector -->
    <controls:HamburgerMenuItemCotentTemplateSelector x:Key="HamburgerMenuItemCotentTemplateSelector">
        <controls:HamburgerMenuItemCotentTemplateSelector.DefaultTemplate>
            <DataTemplate DataType="controls:IHamburgerMenuItem">
                <Grid MinHeight="32" ColumnDefinitions="Auto,*">
                    <Panel Margin="-10,0,0,0" 
                           IsVisible="{Binding Icon, Converter={x:Static ObjectConverters.IsNotNull}}"
                           Width="{Binding CompactPaneLength, RelativeSource={RelativeSource AncestorType=controls:HamburgerMenu}}"></Panel>
                    <ContentControl Grid.Column="0"
                                    Width="{DynamicResource IconElementThemeWidth}"
                                    Margin="-10,0,0,0"
                                    HorizontalAlignment="Center"
                                    VerticalContentAlignment="Center"
                                    HorizontalContentAlignment="Center"
                                    Height="{DynamicResource IconElementThemeHeight}"
                                    Content="{Binding Icon}"
                                    ContentTemplate="{Binding IconTemplate}" />
                    <ContentControl Grid.Column="1"
                                    Content="{Binding Label}"
                                    ContentTemplate="{Binding LabelTemplate}"
                                    VerticalContentAlignment="Center" />
                </Grid>
            </DataTemplate>
        </controls:HamburgerMenuItemCotentTemplateSelector.DefaultTemplate>

        <DataTemplate x:Key="{x:Type controls:HamburgerMenuSeparatorItem}"
                      DataType="controls:HamburgerMenuSeparatorItem">
            <Separator HorizontalAlignment="Stretch" Margin="-6,2,4,2" />
        </DataTemplate>
    </controls:HamburgerMenuItemCotentTemplateSelector>

    <ControlTheme x:Key="HamburgerMenuToggleButon" TargetType="ToggleButton">
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="Content">
            <Template>
                <PathIcon Width="{DynamicResource ControlContentThemeFontSize}"
                          Data="{DynamicResource PhosphorIcons.ListLight}" />
            </Template>
        </Setter>
        <Setter Property="Template">
            <ControlTemplate>
                <ContentPresenter x:Name="PART_ContentPresenter"
                                  HorizontalContentAlignment="Center"
                                  VerticalContentAlignment="Center"
                                  CornerRadius="{DynamicResource ControlCornerRadius}"
                                  Background="{TemplateBinding Background}"
                                  Content="{TemplateBinding Content}"
                                  ContentTemplate="{TemplateBinding ContentTemplate}" />
            </ControlTemplate>
        </Setter>
        <Style Selector="^:checked">
            <Setter Property="Content">
                <Template>
                    <PathIcon Width="{DynamicResource ControlContentThemeFontSize}"
                              Data="{DynamicResource PhosphorIcons.XLight}" />
                </Template>
            </Setter>
        </Style>
        <Style Selector="^:pointerover">
            <Setter Property="Background" Value="{DynamicResource ButtonBackgroundPointerOver}" />
        </Style>
        <Style Selector="^:pressed">
            <Setter Property="Background" Value="{DynamicResource ButtonBackgroundPressed}" />
        </Style>
    </ControlTheme>

    <ControlTheme x:Key="HamburgerMenuNavigationListBox"
                  BasedOn="{StaticResource {x:Type ListBox}}"
                  TargetType="ListBox">
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="ItemContainerTheme" Value="{StaticResource HamburgerMenuNavigationListBoxItem}" />
    </ControlTheme>

    <ControlTheme x:Key="HamburgerMenuNavigationListBoxItem" TargetType="ListBoxItem">
        <Setter Property="IsEnabled" x:DataType="controls:IHamburgerMenuItem" Value="{Binding Enabled}"></Setter>
        <Setter Property="ToolTip.Tip" x:DataType="controls:IHamburgerMenuItem" Value="{Binding Label}"></Setter>
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="ContentTemplate" Value="{StaticResource HamburgerMenuItemCotentTemplateSelector}" />
        <Setter Property="IsVisible">
            <MultiBinding Converter="{x:Static BoolConverters.Or}" x:DataType="controls:IHamburgerMenuItem">
                <Binding Path="IsPaneOpen" RelativeSource="{RelativeSource AncestorType=controls:HamburgerMenu}"></Binding>
                <Binding Path="Icon" Converter="{x:Static ObjectConverters.IsNotNull}"></Binding>
                <Binding Path="!AutoHide" />
            </MultiBinding>
        </Setter>
        <Setter Property="Template">
            <ControlTemplate>
                <Grid x:Name="PART_RootGrid"
                      MinHeight="{TemplateBinding MinHeight}"
                      Background="{TemplateBinding Background}"
                      ColumnDefinitions="10, *">
                    <Rectangle x:Name="PART_Indicator"
                               Width="2"
                               Height="{DynamicResource ControlContentThemeFontSize}"
                               Margin="2"
                               HorizontalAlignment="Center"
                               VerticalAlignment="Center"
                               IsVisible="{TemplateBinding IsEnabled}"
                               RadiusX="2"
                               RadiusY="2" />
                    <ContentPresenter x:Name="PART_ContentPresenter"
                                      Grid.Column="1"
                                      Margin="{TemplateBinding Margin}"
                                      Padding="{TemplateBinding Padding}"
                                      VerticalAlignment="Center"
                                      Content="{TemplateBinding Content}"
                                      ContentTemplate="{TemplateBinding ContentTemplate}" />
                </Grid>
            </ControlTemplate>
        </Setter>

        <Style Selector="^:selected /template/ Rectangle#PART_Indicator">
            <Setter Property="Fill" Value="{DynamicResource SystemControlHighlightListAccentMediumBrush}" />
        </Style>

        <Style Selector="^:pointerover">
            <Setter Property="Background" Value="{DynamicResource SystemControlHighlightListLowBrush}" />
            <Style Selector="^ /template/ Rectangle#PART_Indicator">
                <Setter Property="Fill" Value="{DynamicResource SystemControlHighlightListAccentLowBrush}" />
            </Style>
        </Style>

        <Style Selector="^:pressed">
            <Setter Property="Background" Value="{DynamicResource SystemControlHighlightListMediumBrush}" />
            <Style Selector="^ /template/ Rectangle#PART_Indicator">
                <Setter Property="Fill" Value="{DynamicResource SystemControlHighlightListAccentHighBrush}" />
            </Style>
        </Style>
    </ControlTheme>

    <ControlTheme x:Key="{x:Type controls:HamburgerMenu}" TargetType="controls:HamburgerMenu">
        <Setter Property="Header" Value="{Binding $self.SelectedMenuItem.Label}" />
        <Setter Property="Content" Value="{Binding $self.SelectedMenuItem.Tag}" />
        <Setter Property="HeaderTemplate" Value="{Binding $self.SelectedMenuItem.LabelTemplate}" />
        <Setter Property="PaneBackground" Value="{DynamicResource HamburgerMenuPaneBrush}" />
        <Setter Property="Background" Value="{DynamicResource SystemControlBackgroundAltHighBrush}" />
        <Setter Property="Padding" Value="5"></Setter>
        <Setter Property="OpenPaneLength" Value="200"></Setter>
        <Setter Property="CornerRadius" Value="5,0,0,0"></Setter>
        <Setter Property="IsPaneOpen" Value="False" />
        <Setter Property="Template">
            <ControlTemplate>
                <Grid RowDefinitions="Auto, *">
                    <Border Background="{DynamicResource HamburgerMenuHeaderBrush}"
                            Height="{DynamicResource HamburgerMenuHeaderMinHeight}">
                        <DockPanel>
                            <ToggleButton Width="{TemplateBinding CompactPaneLength}"
                                          MinWidth="20"
                                          VerticalAlignment="Stretch"
                                          DockPanel.Dock="Left"
                                          IsChecked="{TemplateBinding IsPaneOpen,Mode=TwoWay}"
                                          Theme="{DynamicResource HamburgerMenuToggleButon}" />
                            <TransitioningContentControl Content="{TemplateBinding Header}"
                                                         MinHeight="{DynamicResource HamburgerMenuHeaderFontSize}"
                                                         Margin="5"
                                                         VerticalAlignment="Center"
                                                         FontSize="{DynamicResource HamburgerMenuHeaderFontSize}"
                                                         FontWeight="SemiBold"
                                                         Foreground="{DynamicResource HamburgerMenuHeaderForegroundBrush}"
                                                         ContentTemplate="{TemplateBinding HeaderTemplate}" />
                        </DockPanel>
                    </Border>
                    <SplitView Grid.Row="1"
                               Background="{TemplateBinding PaneBackground}"
                               CompactPaneLength="{TemplateBinding CompactPaneLength}"
                               DisplayMode="CompactInline"
                               IsPaneOpen="{TemplateBinding IsPaneOpen}"
                               OpenPaneLength="{TemplateBinding OpenPaneLength}"
                               PaneBackground="Transparent">
                        <SplitView.Pane>
                            <Grid RowDefinitions="*, Auto" RowSpacing="20">
                                <ListBox ItemsSource="{TemplateBinding MenuItems}"
                                         SelectedItem="{TemplateBinding SelectedMenuItem,
                                                                        Mode=TwoWay}"
                                         Theme="{DynamicResource HamburgerMenuNavigationListBox}" />

                                <ListBox Grid.Row="1"
                                         MaxHeight="200"
                                         ItemsSource="{TemplateBinding OptionsMenuItems}"
                                         SelectedItem="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SelectedMenuItem, Mode=TwoWay}"
                                         Theme="{DynamicResource HamburgerMenuNavigationListBox}" />
                            </Grid>
                        </SplitView.Pane>
                        <Border Background="{TemplateBinding Background}"
                                CornerRadius="{TemplateBinding CornerRadius}"
                                Padding="{TemplateBinding Padding}">
                            <TransitioningContentControl Content="{TemplateBinding Content}"
                                                         ContentTemplate="{TemplateBinding ContentTemplate}" />
                        </Border>
                    </SplitView>
                </Grid>
            </ControlTemplate>
        </Setter>
    </ControlTheme>
</ResourceDictionary>